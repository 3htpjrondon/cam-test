## required
url=<%=@metering_url %>
apiKey=<%=@metering_apikey %>

## One of the following is required for SSL 
# sslref=<SSL name of server SSL configuration>
# Or the following 3 properties
trustStore=icp_metering_truststore.jks
trustStorePassword=<%=@metering_keystore_password %>
# httpsProtocol=<TLS protocol, defaults to TLSv1.2>

## proxy only - to use, remove # and define proxy configuration
# proxyUrl=<proxyURL> 
# proxyUser=<proxyUserName> 
# proxyPassword=<proxyPassword>

## optional - to use, remove #
group=<%=@metering_group %>
logData=all
